# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

def shared_pods
	pod "Alamofire"
	pod "Firebase/Core"
	pod "MaterialComponents"
	pod "Firebase"
	pod "FirebaseUI/Auth"
	pod "Firebase/Database"
  pod "Firebase/Storage"
end

target 'InstaClone' do
  use_modular_headers!
	shared_pods
end
